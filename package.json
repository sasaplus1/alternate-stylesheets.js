{
  "name": "alternate-stylesheets",
  "description": "switch alternate stylesheets",
  "version": "0.2.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/mocha": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "espower-typescript": "^10.0.0",
    "husky": "^4.3.8",
    "karma": "^6.2.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-espower-preprocessor": "^1.2.0",
    "karma-mocha": "^2.0.1",
    "karma-power-assert": "^1.0.0",
    "karma-rollup-preprocessor": "^7.0.7",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^2.2.1",
    "rollup": "^2.41.5",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^6.4.9",
    "typescript": "^4.2.3",
    "webpack": "^3.11.0"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "license": "MIT",
  "main": "./index.js",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/alternate-stylesheets.js.git"
  },
  "scripts": {
    "build": "npm run compile && npm run minify",
    "clean": "rm -f ./dist/alternate-stylesheets{,.min}.js",
    "compile": "npm run webpack -- --output-filename alternate-stylesheets.js",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --ext .js,.ts --fix .",
    "fix:prettier": "prettier --write '**/*.+(js|ts)'",
    "fixpack": "npx fixpack",
    "karma": "karma start",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --cache --ext .js,.ts .",
    "lint:prettier": "prettier --check '**/*.+(js|ts)'",
    "minify": "npm run webpack -- -p --output-filename alternate-stylesheets.min.js",
    "postminify": "cp ./dist/alternate-stylesheets.min.js ./docs",
    "serve": "serve ./docs",
    "test": "npm run lint && npm run karma -- --single-run",
    "webpack": "webpack --colors --display-error-details --progress"
  }
}
